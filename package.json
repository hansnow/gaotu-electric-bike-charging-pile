    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "backtest": "tsx scripts/backtest-idle-alert.ts"
  },
  "keywords": [
    "cloudflare-workers",
    "electric-bike",
    "charging-pile",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.13.1",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241218.0",
    "@types/node": "^22.10.2",
    "@vitest/coverage-v8": "^3.2.4",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4",
    "wrangler": "^4.42.2"
  }
}
